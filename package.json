{
  "name": "@justkd/uidmanager",
  "author": "Cadence Holmes, @justkd",
  "version": "1.1.0",
  "license": "MIT",
  "description": "",
  "keywords": [
    "id",
    "uid",
    "uuid",
    "unique",
    "identifier",
    "manage",
    "manager",
    "storage",
    "validator",
    "validate",
    "validation",
    "generate",
    "generator",
    "map",
    "mapper",
    "mapped",
    "npm"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/justkd/uidmanager"
  },
  "main": "lib/index",
  "types": "lib/index",
  "files": [
    "lib",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "test": "npx mocha --require mocha-dom",
    "watch": "npx mocha -w --require mocha-dom",
    "build": "npm run build:lib && npm run build:docs && npm run minify",
    "build:lib": "npx tsc -p .",
    "build:docs": "npx typedoc --out docs src/index.ts --cleanOutputDir",
    "minify": "npx minify-all-js ./lib && npx minify-all-js ./docs",
    "publish": "npm publish"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --cache --fix"
    ]
  },
  "mocha": {
    "extension": [
      "ts"
    ],
    "watch-files": [
      "src/**/*.ts",
      "test/**/*.ts"
    ],
    "require": "ts-node/register"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "chai": "^4.3.7",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.25.2",
    "jsdom": "^17.0.0",
    "lint-staged": "^13.2.3",
    "minify-all-js": "^0.1.9",
    "mocha": "^10.2.0",
    "mocha-dom": "^1.0.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.8",
    "typedoc-plugin-markdown": "^3.15.3",
    "typescript": "^5.1.6"
  }
}
